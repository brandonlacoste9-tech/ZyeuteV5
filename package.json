  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx backend/index.ts",
    "build": "vite build && esbuild backend/index.ts --bundle --platform=node --target=node20 --format=cjs --outfile=dist/index.cjs --external:sharp --external:pg --external:vite --external:./vite.js --define:process.env.NODE_ENV=\\\"production\\\"",
    "build:vercel": "vite build",
    "start": "node dist/index.cjs",
    "check": "tsc",
    "check:env": "tsx scripts/check-env.ts",
    "setup:external": "node scripts/setup-external-tools.js",
    "lint": "tsc --noEmit && eslint .",
    "test": "vitest run",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "migrate": "tsx ./scripts/run-migration.ts",
    "predeploy": "npm run migrate",
    "worker:video": "tsx backend/workers/videoProcessor.ts",
    "preflight": "npm run check && npm run lint && npm run build && npm run test",
    "worker": "tsx backend/startWorker.ts",
    "db:push": "drizzle-kit push",
    "db:deploy": "tsx scripts/db-deploy.ts",
